def q8(strng):
    '''
    Given a sentence as a string with words being separated by a single space,
    return the length of the shortest word.
    '''
    words = strng.split()
    minlen = len(words[0])
    for word in words:
        if len(word) < minlen:
            minlen = len(word)
    return minlen


    short = min(strng.split(), key=len)
    length = len(short)
    return length


    pass

