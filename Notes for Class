def q7(filename):
     
    TLO: 112-SCRPY005, LSA 1
    Given a filename, open the file and return the length of the first line 
    in the file excluding the line terminator

  with open(filename) as fp:
    x = fp.readline()
    y = len(x) - 1
    return y

  with open(filename) as fp:
    return len(fp.readline()) - 1

